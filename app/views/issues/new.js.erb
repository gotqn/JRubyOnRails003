var content_html = [];
var footer_html = [];

content_html.push('<%=j render template: 'issues/new.html.erb' %>');

footer_html.push('<%= j link_to 'Back to Issues', issues_path, remote:true %>');

$('.b-content-container-content').html(content_html.join(''));
