<%= simple_form_for(@security_users_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :faculty_number %>
    <%= f.input :egn %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :gender %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :country %>
    <%= f.input :gsm %>
    <%= f.input :skype %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
